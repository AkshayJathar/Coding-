#include <stdio.h>
#define MAX 50
void frequency(int [] , int);
int main()
{
    int a[MAX] , num , rem ,i;
    printf("enter the number");
    scanf("%d",&num);
    frequency(a,num);
    return (0);
}
void frequency(int a[] , int num)
{
    int i, rem;
    while(num>0)
    {
        rem=num%10;
        a[rem]=a[rem]+1;
        num=num/10;
    }
     for(i=0;i<10;i++)
     {
         if(a[i]!=0)
         printf(" %d has appeared %d times \n", i, a[i]);
         
     }
}

