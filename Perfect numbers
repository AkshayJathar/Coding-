#include <stdio.h>

int main()
{
    int n,i ,range,sum;
    printf("enter the range");
    scanf("%d",&range);
    
    for(n=1 ;n<=range;n++)
    {
    sum=0;
    
    for(i=1;i<n;i++)
    {
        if(n%i==0)
        {
        sum=sum+i;
        }
    }
    if(n==sum)
   printf("perfect no are %d\n",n);
   
}
}
